{
  "name": "semantic-release-my-test-project",
  "version": "1.0.0",
  "description": "semantic release task",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VildanovN/semantic-release.git"
  },
  "author": "VildanovN",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VildanovN/semantic-release/issues"
  },
  "homepage": "https://github.com/VildanovN/semantic-release#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "semantic-release": "^19.0.5"
  },
  "branches": [{ "name": "master", "prerelease": true}],
    "plugins": [
        [
            "@semantic-release/commit-analyzer", {
                "preset": "conventionalcommits"
            } 
        ],
        [
        "@semantic-release/release-notes-generator", {
                "preset": "conventionalcommits"
            }
        ], 
        [ 
            "@semantic-release/changelog",
            {
                "changelogFile": "docs/CHANGELOG.md"
            }
        ],
		"@semantic-release/npm",
        [
            "@semantic-release/git",
            {
                "assets": ["dist/**/package.json"],
				"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
            }
        ],
		"@semantic-release/github"
    ]
}
